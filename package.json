{
  "name": "nestjs-starter",
  "version": "0.0.0",
  "description": "Gal Starter",
  "author": "Gal Batz",
  "license": "MIT",
  "scripts": {
    "webpack": "webpack --config webpack.config.js",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f src/ormconfig.ts",
    "typeorm-dev:cli": "ts-node ./node_modules/typeorm/cli -f src/ormconfig.dev.ts",
    "hmr": "node dist/server",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@godaddy/terminus": "4.1.0",
    "@nestjs/common": "6.8.2",
    "@nestjs/core": "6.8.2",
    "@nestjs/passport": "6.0.0",
    "@nestjs/platform-express": "6.1.0",
    "@nestjs/swagger": "3.1.0",
    "@nestjs/terminus": "6.1.0",
    "@nestjs/typeorm": "6.2.0",
    "@types/jsonwebtoken": "8.3.4",
    "@types/pg": "^7.14.3",
    "asn1.js": "5.0.1",
    "async": "2.6.1",
    "bcryptjs": "2.4.3",
    "cache-manager": "2.9.0",
    "cache-manager-redis-store": "1.5.0",
    "class-transformer": "0.2.0",
    "class-validator": "0.9.1",
    "csurf": "1.9.0",
    "dotenv": "6.2.0",
    "geoip-lite": "1.3.5",
    "helmet": "3.15.0",
    "joi": "14.3.1",
    "jsonwebtoken": "8.4.0",
    "murmurhash3js": "3.0.1",
    "mysql": "2.16.0",
    "node-pre-gyp": "0.12.0",
    "passport": "0.4.0",
    "passport-http-bearer": "1.0.1",
    "pg": "^8.2.1",
    "rand-token": "0.4.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "2.6.3",
    "rxjs": "6.3.3",
    "swagger-ui-express": "4.0.2",
    "traverse": "0.6.6",
    "typeorm": "0.2.19",
    "typescript": "3.6.4",
    "useragent": "2.3.0"
  },
  "devDependencies": {
    "@nestjs/testing": "5.7.4",
    "@types/bcryptjs": "2.4.2",
    "@types/dotenv": "6.1.1",
    "@types/express": "4.16.1",
    "@types/helmet": "0.0.44",
    "@types/jest": "23.3.13",
    "@types/joi": "14.3.0",
    "@types/node": "10.12.18",
    "@types/supertest": "2.0.7",
    "jest": "23.6.0",
    "nodemon": "1.18.9",
    "prettier": "1.16.1",
    "supertest": "3.4.2",
    "ts-jest": "23.10.5",
    "ts-loader": "4.5.0",
    "ts-node": "7.0.1",
    "tsconfig-paths": "3.7.0",
    "tslint": "5.11.0",
    "webpack": "4.29.0",
    "webpack-cli": "3.2.1",
    "webpack-node-externals": "1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
